<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.RealEstateAgency_1" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <item name="colorPrimary">#1C3A75</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorAccent">#1C3A75</item> <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">@color/white</item>
        <item name="android:windowLightStatusBar">true</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="MySearchViewStyle" parent="Widget.AppCompat.SearchView">
        <item name="searchHintIcon">@null</item>
    </style>


    <style name="MyRatingBar" parent="@android:style/Widget.RatingBar">
        <item name="android:minHeight">12dp</item>
        <item name="android:maxHeight">5dp</item>
        <item name="colorControlNormal">@color/grey_col</item>
        <item name="colorControlActivated">@color/yelow</item>
    </style>
</resources>