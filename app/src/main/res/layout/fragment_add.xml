<?xml version="1.0" encoding="utf-8"?>
<androidx.coordinatorlayout.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".ui.fragment.add.AddFragment">

        <androidx.core.widget.NestedScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <androidx.cardview.widget.CardView
                    android:id="@+id/card_image"
                    android:layout_width="360dp"
                    android:layout_height="200dp"
                    android:layout_marginStart="40dp"
                    android:layout_marginTop="30dp"
                    android:layout_marginEnd="40dp"
                    app:cardCornerRadius="20dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent">

                    <androidx.recyclerview.widget.RecyclerView
                        android:id="@+id/vp_image"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                        tools:listitem="@layout/add_rv_item" />

                    <TextView
                        android:id="@+id/dfgh"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Фото"
                        android:textSize="22sp"
                        android:layout_marginStart="155dp"
                        android:layout_marginTop="80dp"/>
                </androidx.cardview.widget.CardView>

                <com.google.android.material.button.MaterialButton
                    android:id="@+id/tvb"
                    android:layout_width="70dp"
                    android:layout_height="70dp"
                    android:layout_marginEnd="10dp"
                    android:layout_marginTop="10dp"
                    app:cornerRadius="138dp"
                    android:paddingStart="22dp"
                    android:backgroundTint="#5671FD"
                    app:icon="@drawable/baseline_add_a_photo_24"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/card_image"/>

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/con"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="20dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/card_image">
                    <com.google.android.material.textfield.TextInputLayout
                        android:id="@+id/wern"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        android:layout_marginStart="65dp"
                        android:hint="Id apartment"
                        android:layout_marginTop="5dp"
                        android:layout_marginEnd="105dp">

                        <com.google.android.material.textfield.TextInputEditText
                            android:id="@+id/et_id"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:imeOptions="actionSearch"
                            android:inputType="text"
                            android:paddingStart="15dp"
                            android:textColor="#120D0D"
                            android:textSize="14sp"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />
                    </com.google.android.material.textfield.TextInputLayout>

                    <com.google.android.material.textfield.TextInputLayout
                        android:id="@+id/wern_qwe"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@id/wern"
                        android:layout_marginStart="65dp"
                        android:hint="Логин"
                        android:layout_marginTop="5dp"
                        android:layout_marginEnd="105dp">

                        <com.google.android.material.textfield.TextInputEditText
                            android:id="@+id/et_idf"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:imeOptions="actionSearch"
                            android:inputType="text"
                            android:paddingStart="15dp"
                            android:textColor="#120D0D"
                            android:textSize="14sp"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />
                    </com.google.android.material.textfield.TextInputLayout>

                    <com.google.android.material.textfield.TextInputLayout
                        android:id="@+id/wern_kkol"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@id/wern_qwe"
                        android:layout_marginStart="65dp"
                        android:hint="Пароль"
                        android:layout_marginTop="5dp"
                        android:layout_marginEnd="105dp">

                        <com.google.android.material.textfield.TextInputEditText
                            android:id="@+id/et_idg"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:imeOptions="actionSearch"
                            android:inputType="text"
                            android:paddingStart="15dp"
                            android:textColor="#120D0D"
                            android:textSize="14sp"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />
                    </com.google.android.material.textfield.TextInputLayout>

                    <com.google.android.material.button.MaterialButton
                        android:id="@+id/btn_hjko"
                        android:layout_width="match_parent"
                        android:layout_height="45dp"
                        android:layout_marginStart="85dp"
                        android:layout_marginTop="5dp"
                        android:layout_marginEnd="120dp"
                        android:text="Достать токен"
                        android:textAllCaps="false"
                        app:cornerRadius="10dp"
                        android:backgroundTint="#5671FD"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@id/wern_kkol" />


                    <com.google.android.material.button.MaterialButton
                        android:id="@+id/btnPk"
                        android:layout_width="match_parent"
                        android:layout_height="45dp"
                        android:layout_marginStart="85dp"
                        android:layout_marginTop="5dp"
                        android:layout_marginEnd="120dp"
                        android:text="Опубликовать"
                        android:textAllCaps="false"
                        app:cornerRadius="10dp"
                        android:backgroundTint="#5671FD"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@id/btn_hjko" />


                    <com.google.android.material.textfield.TextInputLayout
                        android:id="@+id/qwety"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="25dp"
                        android:layout_marginTop="10dp"
                        android:layout_marginEnd="25dp"
                        style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                        app:layout_constraintEnd_toEndOf="parent"
                        android:hint="Teкст"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@id/btnPk">

                        <com.google.android.material.textfield.TextInputEditText
                            android:id="@+id/et_text"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:imeOptions="actionSearch"
                            android:inputType="text"
                            android:paddingStart="15dp"
                            android:textSize="14sp"
                            android:textColor="#120D0D"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@id/btnPk" />

                    </com.google.android.material.textfield.TextInputLayout>

                    <com.google.android.material.textfield.TextInputLayout
                        android:id="@+id/hjk"
                        android:layout_width="150dp"
                        android:layout_height="wrap_content"
                        style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                        android:hint="Адрес"
                        android:layout_marginStart="25dp"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@id/qwety"
                        android:layout_marginTop="20dp">

                        <com.google.android.material.textfield.TextInputEditText
                            android:id="@+id/et_location"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:imeOptions="actionSearch"
                            android:inputType="text"
                            android:paddingStart="15dp"
                            android:textSize="14sp"
                            android:textColor="#120D0D" />

                    </com.google.android.material.textfield.TextInputLayout>

                    <com.google.android.material.textfield.TextInputLayout
                        android:id="@+id/fghj"
                        android:layout_width="170dp"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="20dp"
                        style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                        android:layout_marginEnd="25dp"
                        android:hint="Квадратный метp"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toBottomOf="@id/qwety">

                        <com.google.android.material.textfield.TextInputEditText
                            android:id="@+id/et_km"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:textSize="13sp"
                            android:imeOptions="actionSearch"
                            android:inputType="text"
                            android:textColor="#120D0D"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toEndOf="@id/hjk"
                            app:layout_constraintTop_toBottomOf="@id/qwety" />

                    </com.google.android.material.textfield.TextInputLayout>

                    <com.google.android.material.textfield.TextInputLayout
                        android:id="@+id/jkl"
                        android:layout_width="150dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="25dp"
                        android:layout_marginTop="20dp"
                        style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                         app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@id/hjk"
                        android:hint="Количество комнат">

                        <com.google.android.material.textfield.TextInputEditText
                            android:id="@+id/et_room"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:paddingStart="15dp"
                            android:imeOptions="actionSearch"
                            android:inputType="text"
                            android:textColor="@color/black"
                            android:textSize="14sp" />

                    </com.google.android.material.textfield.TextInputLayout>


                    <androidx.recyclerview.widget.RecyclerView
                        android:id="@+id/rv_room"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="30dp"
                        android:orientation="horizontal"
                        android:visibility="gone"
                        app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@id/jkl"
                        tools:listitem="@layout/item_add" />

                    <com.google.android.material.textfield.TextInputLayout
                        android:id="@+id/ghj"
                        android:layout_width="190dp"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="20dp"
                        style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                        android:layout_marginEnd="25dp"
                        android:hint="Номер телефона"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toBottomOf="@id/hjk">

                        <com.google.android.material.textfield.TextInputEditText
                            android:id="@+id/et_phone"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:paddingStart="15dp"
                            android:imeOptions="actionSearch"
                            android:inputType="text"
                            android:textSize="14sp"
                            android:textColor="#120D0D"
                            android:textColorHint="#120D0D"/>

                    </com.google.android.material.textfield.TextInputLayout>

                    <com.google.android.material.textfield.TextInputLayout
                        android:id="@+id/fgh"
                        android:layout_width="170dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="25dp"
                        android:layout_marginTop="20dp"
                        android:layout_marginEnd="10dp"
                         app:layout_constraintStart_toStartOf="parent"
                        android:hint="Тип недвижимости"
                        app:layout_constraintTop_toBottomOf="@id/rv_room"
                        style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox">

                        <com.google.android.material.textfield.TextInputEditText
                            android:id="@+id/et_dil"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:paddingStart="15dp"
                            android:textColor="#120D0D"
                            android:imeOptions="actionSearch"
                            android:inputType="text"
                            android:textSize="14sp" />
                    </com.google.android.material.textfield.TextInputLayout>

                    <androidx.recyclerview.widget.RecyclerView
                        android:id="@+id/rv_type"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="25dp"
                        android:orientation="horizontal"
                        android:visibility="gone"
                        app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@id/fgh"
                        tools:listitem="@layout/item_add" />

                    <com.google.android.material.textfield.TextInputLayout
                        android:id="@+id/bnm"
                        android:layout_width="170dp"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="20dp"
                        android:hint="Цена только цифры"
                        android:layout_marginEnd="25dp"
                        style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toBottomOf="@id/rv_room">

                        <com.google.android.material.textfield.TextInputEditText
                            android:id="@+id/et_san"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:paddingStart="15dp"
                            android:textColor="#120D0D"
                            android:imeOptions="actionSearch"
                            android:inputType="text"
                            android:textSize="14sp" />
                    </com.google.android.material.textfield.TextInputLayout>


                    <com.google.android.material.textfield.TextInputLayout
                        android:id="@+id/ghk"
                        android:layout_width="160dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="25dp"
                        android:layout_marginTop="20dp"
                        android:hint="Валюта"
                        app:layout_constraintStart_toStartOf="parent"
                        style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                        app:layout_constraintTop_toBottomOf="@id/rv_type">

                        <com.google.android.material.textfield.TextInputEditText
                            android:id="@+id/et_er"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:paddingStart="15dp"
                            android:textColor="#120D0D"
                            android:imeOptions="actionSearch"
                            android:inputType="text"
                            android:textSize="14sp" />

                    </com.google.android.material.textfield.TextInputLayout>

                    <androidx.recyclerview.widget.RecyclerView
                        android:id="@+id/rv_valute"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="20dp"
                        android:orientation="horizontal"
                        android:visibility="gone"
                        app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@id/ghk"
                        tools:listitem="@layout/item_add" />


                    <com.google.android.material.textfield.TextInputLayout
                        android:id="@+id/klop"
                        style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                        android:layout_width="160dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="25dp"
                        android:layout_marginTop="20dp"
                        android:hint="Серия"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@id/rv_valute">

                        <com.google.android.material.textfield.TextInputEditText
                            android:id="@+id/et_dv_zxx"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:paddingStart="15dp"
                            android:textColor="#120D0D"
                            android:imeOptions="actionSearch"
                            android:inputType="text"
                            android:textSize="14sp"/>
                    </com.google.android.material.textfield.TextInputLayout>

                    <Switch
                        android:id="@+id/et_l"
                        android:layout_width="wrap_content"
                        android:layout_height="40dp"
                        android:layout_marginStart="15dp"
                        android:layout_marginTop="20dp"
                        android:layout_marginEnd="40dp"
                        android:text="Лучший "
                        android:textColor="#787575"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toBottomOf="@id/rv_series"
                        tools:ignore="UseSwitchCompatOrMaterialXml" />

                    <androidx.recyclerview.widget.RecyclerView
                        android:id="@+id/rv_series"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="20dp"
                        android:orientation="horizontal"
                        android:visibility="gone"
                        app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@id/klop"
                        tools:listitem="@layout/item_add" />

                    <com.google.android.material.textfield.TextInputLayout
                        android:id="@+id/asd"
                        android:layout_width="160dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="15dp"
                        android:layout_marginTop="20dp"
                        android:layout_marginEnd="25dp"
                        android:hint="Документы"
                        style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toBottomOf="@id/rv_valute" >

                        <com.google.android.material.textfield.TextInputEditText
                            android:id="@+id/et_dv_x"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:paddingStart="15dp"
                            android:imeOptions="actionSearch"
                            android:inputType="text"
                            android:textColor="#120D0D"
                            android:textSize="14sp" />

                    </com.google.android.material.textfield.TextInputLayout>

                    <androidx.recyclerview.widget.RecyclerView
                        android:id="@+id/rv_docum"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="225dp"
                        app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                        android:orientation="horizontal"
                        android:visibility="gone"
                        app:layout_constraintStart_toStartOf="@id/asd"
                        app:layout_constraintTop_toBottomOf="@id/asd"
                        tools:listitem="@layout/item_add" />

                    <com.google.android.material.textfield.TextInputLayout
                        android:id="@+id/lpo"
                        android:layout_width="160dp"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="20dp"
                        android:layout_marginEnd="25dp"
                        android:hint="Комуникации"
                        app:layout_constraintEnd_toEndOf="parent"
                        style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                        app:layout_constraintTop_toBottomOf="@id/rv_type">

                        <com.google.android.material.textfield.TextInputEditText
                            android:id="@+id/et_df"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:imeOptions="actionSearch"
                            android:paddingStart="15dp"
                            android:textColor="#120D0D"
                            android:inputType="text"
                            android:textSize="14dp" />

                    </com.google.android.material.textfield.TextInputLayout>

                    <com.google.android.material.textfield.TextInputLayout
                        android:id="@+id/yyu"
                        style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                        android:layout_width="160dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="25dp"
                        android:layout_marginTop="20dp"
                        android:hint="Регион"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@id/rv_series" >

                        <com.google.android.material.textfield.TextInputEditText
                            android:id="@+id/et_dav"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:paddingStart="15dp"
                            android:imeOptions="actionSearch"
                            android:inputType="text"
                            android:textColor="#120D0D"
                            android:textSize="14sp" />
                    </com.google.android.material.textfield.TextInputLayout>

                    <androidx.recyclerview.widget.RecyclerView
                        android:id="@+id/rv_regifn"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="20dp"
                        android:orientation="horizontal"
                        android:visibility="gone"
                        app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                        app:layout_constraintStart_toStartOf="@id/yyu"
                        app:layout_constraintTop_toBottomOf="@id/yyu"
                        tools:listitem="@layout/item_add" />


                    <com.google.android.material.textfield.TextInputLayout
                        android:id="@+id/et_layout_title"
                        style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="15dp"
                        android:layout_marginTop="20dp"
                        android:layout_marginEnd="15dp"
                        android:hint="Информация"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@id/rv_regifn">

                        <com.google.android.material.textfield.TextInputEditText
                            android:id="@+id/edit_in"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:gravity="top"
                            android:imeOptions="actionSearch"
                            android:inputType="text"
                            android:textColor="#120D0D"
                            android:textSize="12sp" />
                    </com.google.android.material.textfield.TextInputLayout>
                </androidx.constraintlayout.widget.ConstraintLayout>

                <TextView
                    android:id="@+id/tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="30dp"
                    android:layout_marginTop="20dp"
                    android:text="Ваши данные"
                    android:textColor="@color/black"
                    android:textSize="26sp"
                    android:textStyle="bold"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/con" />

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/con_two"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="20dp"
                    android:layout_marginTop="20dp"
                    android:layout_marginEnd="20dp"
                    android:background="#FFFF"
                    android:layout_marginBottom="30dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.0"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/tv">

                    <com.google.android.material.textfield.TextInputLayout
                        android:id="@+id/vbt"
                        android:layout_width="270dp"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="20dp"
                        app:layout_constraintEnd_toEndOf="parent"
                        android:hint="Login"
                        style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent">

                        <com.google.android.material.textfield.TextInputEditText
                            android:id="@+id/et_layout_fio"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:textColor="@color/black"
                            android:paddingStart="15dp"
                            android:imeOptions="actionSearch"
                            android:inputType="text"
                            android:textSize="14sp"/>
                    </com.google.android.material.textfield.TextInputLayout>

                    <com.google.android.material.textfield.TextInputLayout
                        android:id="@+id/lki"
                        android:layout_width="270dp"
                        android:layout_height="wrap_content"
